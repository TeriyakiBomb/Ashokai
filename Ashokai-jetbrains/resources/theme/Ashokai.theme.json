{
  "name": "Ashokai",
  "author": "YourCompany",
  "editorScheme": "/Ashokai.xml",
  "dark": true,
  "colors": {
    "bg": "#12151B",
    "bg2": "#171A21",
    "borders": "#0e0e14",
    "editorBG": "#1D222A",
    "text": "#DFE8F0",
    "button1" : "#222833",
    "button2" : "#2b3240"
  },
  "ui": {
    "*" : {
      "background": "bg",
      "foreground": "text",
      "Borders.color" : "borders",
      "windowText": "text",
      "Button.foreground": "text",
      "Button.default.endBackground": "button1",
      "Button.default.startBackground": "button2",
      "TabbedPane.underlineColor": "#ff0000"
    },

    "Panel" : {
      "background" : "bg2"

    },

    "ToolWindow":{
      "background": "bg2",
      "Header.background": "#ff0000",
      "Header.inactiveBackground": "bg2",
      "HeaderTab.hoverBackground": "bg2"
    },
      "MainWindow.Tab.foreground": "#ff0000",
    "EditorTabs": {
      "selectedForeground": "text",
      "underlinedTabBackground": "editorBG",
      "background": "bg",
      "underlineColor": "editorBG"
    },
    "ComboBox": {
      "background" : "bg2",
      "nonEditableBackground": "bg2",
      "ArrowButton": {
        "iconColor": "text",
        "disabledIconColor": "bg",
        "nonEditableBackground": "button1"
      }
    },
    "CheckBox": {
      "background" : "bg2",
      "select": "#800000",
      "Checkbox.Foreground.Selected.Dark" : "#800000"
    }
  },
  "icons": {
    "ColorPalette": {
      "Checkbox.Background.Default.Dark": "bg",
      "Checkbox.Border.Default.Dark": "button2",
      "Checkbox.Foreground.Selected": "text",
      "Checkbox.Foreground.Selected.Dark": "text",
      "Checkbox.Background.Default": "bg",
      "Checkbox.Background.Disabled": "bg",
      "Checkbox.Background.Disabled.Dark": "bg"
    }
  }
}